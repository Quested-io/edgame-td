{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.quested.io/ https://quested.io/ http://localhost:3000/"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://quested.io/ http://localhost:3000/"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
